        <url-pattern>/retrieveDeliveryRecord</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>retrieveAllOrder</servlet-name>
        <url-pattern>/retrieveAllOrder</url-pattern>
