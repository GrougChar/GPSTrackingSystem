    </servlet>
    <servlet>
        <servlet-name>deliverOrder</servlet-name>
        <servlet-class>rp.sg.GPSTrackingController.deliverOrder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>storeNewOrder</servlet-name>
        <servlet-class>rp.sg.GPSTrackingController.storeNewOrder</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>locationTracking</servlet-name>
        <url-pattern>/locationTracking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>locationTrackingJackson</servlet-name>
        <url-pattern>/locationTrackingJackson</url-pattern>
