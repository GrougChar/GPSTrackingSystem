    <servlet>
        <servlet-name>retrieveAllDriver</servlet-name>
        <servlet-class>rp.sg.GPSTrackingService.retrieveAllDriver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>retrieveAllVehicle</servlet-name>
        <servlet-class>rp.sg.GPSTrackingService.retrieveAllVehicle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>retrieveDeliveryRecord</servlet-name>
